p1_MODE=IN
p1_TYPE=FILE
p1_default_value=
p1_description=A zip file containing CEL files or zipped CEL files
p1_fileFormat=zip
p1_name=input.filename
p1_optional=
p1_prefix_when_specified=
p1_type=java.io.File
p1_value=

p2_default_value=<input.filename_basename>
p2_description=The base name of the output file
p2_name=output.file
p2_optional=
p2_prefix_when_specified=
p2_type=java.lang.String
p2_value=

p3_default_value=
p3_description=The method to use
p3_name=method
p3_optional=
p3_prefix_when_specified=
p3_type=java.lang.String
p3_value=RMA;MAS5

p4_default_value=
p4_description=(RMA only) Whether to normalize data using quantile normalization
p4_name=quantile.normalization
p4_optional=on
p4_prefix_when_specified=
p4_type=java.lang.String
p4_value=yes;no

p5_default_value=
p5_description=(RMA only) Whether to background correct using RMA background correction
p5_name=background.correct
p5_optional=on
p5_prefix_when_specified=
p5_type=java.lang.String
p5_value=yes;no

p6_default_value=
p6_description=(MAS5 only) Value at which all arrays will be scaled to
p6_name=scale.value
p6_optional=on
p6_prefix_when_specified=
p6_type=java.lang.String
p6_value=

p7_default_value=
p7_description=(MAS5 only) Whether to compute Present/Absent calls
p7_name=compute.present.absent.calls
p7_optional=on
p7_prefix_when_specified=
p7_type=java.lang.String
p7_value=yes=yes (create res file);no=no (create gct file)

p8_default_value=
p8_description=The normalization method to apply after expression values are computed
p8_name=normalization
p8_optional=on
p8_prefix_when_specified=
p8_type=java.lang.String
p8_value=\=;mean scaling;median scaling

p9_default_value=
p9_description=(mean or median scaling only) The sample index (starting at 1) to be used as a reference for normalization
p9_name=sample.index
p9_optional=on
p9_prefix_when_specified=
p9_type=java.lang.Integer
p9_value=

