commandLine=<R> <libdir>expr.R create.expression.file -i<input.filename> -o<output.file> -m<method> -q<quantile.normalization> -b<background.correct> -s<scale.value> -c<compute.present.absent.calls> -n<normalization> -x<sample.index> -l<libdir> -f<clm.filename>
